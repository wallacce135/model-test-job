{
  "compilerOptions": {
    "target": "es2016",
    "outDir": "./dist",
    /* Modules */
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,
    "noImplicitAny": true,
    "skipLibCheck": true                                 /* Skip type checking all .d.ts files. */
  }
}
